<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".main.home.HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clParent"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/layout_common_app_bar"
            layout="@layout/layout_common_app_bar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:id="@+id/cdlLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/layout_common_app_bar">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clTopContent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingBottom="100dp">

                <ImageView
                    android:id="@+id/ivProfile"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_marginStart="@dimen/side_margin"
                    android:layout_marginTop="30dp"
                    android:background="@drawable/ic_profile_img"

                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tvNickname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/side_margin"

                    android:fontFamily="@font/pretendard_medium"
                    android:includeFontPadding="false"
                    android:text="김동동 님,"
                    android:textColor="@color/gray1_42"
                    android:textSize="@dimen/H4_M500_18_auto"

                    app:layout_constraintBottom_toTopOf="@id/tvToday"
                    app:layout_constraintStart_toEndOf="@id/ivProfile"
                    app:layout_constraintTop_toTopOf="@id/ivProfile" />

                <TextView
                    android:id="@+id/tvToday"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/pretendard_bold"
                    android:includeFontPadding="false"
                    android:text="오늘 하루"
                    android:textColor="@color/black1_20"
                    android:textSize="@dimen/H3_B700_20_h30"

                    app:layout_constraintBottom_toBottomOf="@id/ivProfile"
                    app:layout_constraintStart_toStartOf="@id/tvNickname"
                    app:layout_constraintTop_toBottomOf="@id/tvNickname" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:fontFamily="@font/pretendard_medium"
                    android:includeFontPadding="false"
                    android:text="도 힘내세요!"
                    android:textColor="@color/gray1_42"
                    android:textSize="@dimen/H4_M500_18_auto"

                    app:layout_constraintBottom_toBottomOf="@id/tvToday"
                    app:layout_constraintStart_toEndOf="@id/tvToday"
                    app:layout_constraintTop_toTopOf="@id/tvToday" />

                <androidx.viewpager2.widget.ViewPager2
                    android:id="@+id/vpBanner"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"

                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ivProfile"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.core.widget.NestedScrollView
                android:id="@+id/nsvBottomSheetContent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:background="@color/transparent"
                android:fillViewport="true"
                android:overScrollMode="never"
                android:scrollbars="none"
                app:behavior_hideable="false"
                app:behavior_peekHeight="530dp"
                app:layout_behavior="@string/bottom_sheet_behavior">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/shadow_top_corner_radius_15"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:elevation="1dp">

                    <View
                        android:id="@+id/viewHandler"
                        android:layout_width="40dp"
                        android:layout_height="4dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/bg_rounded_corner_radius_20"
                        android:backgroundTint="@color/gray6_F5"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <!-- Layout 한 줄 명언 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clPhrase"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/side_margin"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/bg_rounded_corner_radius_10"
                        android:backgroundTint="@color/gray6_F5"
                        android:padding="10dp"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/viewHandler">

                        <ImageView
                            android:id="@+id/ivQuote"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:src="@drawable/ic_quote"

                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/tvWiseSaying"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="7dp"

                            android:fontFamily="@font/pretendard_medium"
                            android:text="인내, 끈기, 그리고 땀은 성공을 위한 최고의 조합이다."
                            android:textColor="@color/gray1_42"
                            android:textSize="@dimen/M500_14_h20"

                            android:maxLines="1"
                            android:ellipsize="marquee"
                            android:marqueeRepeatLimit="marquee_forever"
                            android:scrollHorizontally="true"
                            android:focusable="true"
                            android:focusableInTouchMode="true"

                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/ivQuote"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <!-- Layout End 한 줄 명언 -->

                    <TextView
                        android:id="@+id/tvWeekWearStatus"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="40dp"

                        android:fontFamily="@font/pretendard_bold"
                        android:text="이번 주 착용 현황"
                        android:textColor="@color/gray1_42"
                        android:textSize="@dimen/B700_16_auto"

                        app:layout_constraintStart_toStartOf="@id/clPhrase"
                        app:layout_constraintTop_toBottomOf="@id/clPhrase" />

                    <com.kizitonwose.calendar.view.WeekCalendarView
                        android:id="@+id/wcvWeekWearStatus"
                        android:layout_width="0dp"
                        android:layout_height="60dp"
                        android:layout_marginHorizontal="@dimen/side_margin"
                        android:layout_marginTop="15dp"
                        android:clickable="false"
                        android:focusable="false"
                        app:cv_dayViewResource="@layout/layout_calendar_day_of_week_cell"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvWeekWearStatus" />

                    <View
                        android:id="@+id/viewLineLegend"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginBottom="10dp"
                        android:background="@drawable/shape_oval"
                        android:backgroundTint="@color/primary_normal_light"

                        app:layout_constraintBottom_toTopOf="@id/lcWeekStatusChart"
                        app:layout_constraintStart_toStartOf="@id/lcWeekStatusChart" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"

                        android:fontFamily="@font/pretendard_semibold"
                        android:includeFontPadding="false"
                        android:text="내 착용 시간"
                        android:textColor="@color/gray1_42"
                        android:textSize="@dimen/M500_12_auto"

                        app:layout_constraintBottom_toBottomOf="@id/viewLineLegend"
                        app:layout_constraintStart_toEndOf="@id/viewLineLegend"
                        app:layout_constraintTop_toTopOf="@id/viewLineLegend" />


                    <com.github.mikephil.charting.charts.LineChart
                        android:id="@+id/lcWeekStatusChart"
                        android:layout_width="0dp"
                        android:layout_height="170dp"
                        android:layout_marginTop="70dp"

                        app:layout_constraintBottom_toTopOf="@id/ivBannerChatBot"
                        app:layout_constraintEnd_toEndOf="@id/wcvWeekWearStatus"
                        app:layout_constraintStart_toStartOf="@id/wcvWeekWearStatus"
                        app:layout_constraintTop_toBottomOf="@id/wcvWeekWearStatus" />

                    <!-- Layout 챗봇 배너 -->
                    <ImageView
                        android:id="@+id/ivBannerChatBot"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/side_margin"
                        android:layout_marginVertical="@dimen/side_margin_2x"
                        android:adjustViewBounds="true"
                        android:clickable="true"
                        android:src="@drawable/img_banner_chat_bot"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/lcWeekStatusChart" />
                    <!-- Layout End 챗봇 배너 -->

                    <!-- Layout 주변 인기치과 레이아웃 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clDentalMagazine"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:paddingVertical="@dimen/side_margin_2x"
                        android:background="@color/white"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ivBannerChatBot">

                        <TextView
                            android:id="@+id/tvTodayMagazine"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:layout_marginStart="@dimen/side_margin"

                            android:fontFamily="@font/pretendard_extrabold"
                            android:includeFontPadding="false"
                            android:text="오늘의 매거진"
                            android:textColor="@color/black1_20"
                            android:textSize="@dimen/H4_M500_18_auto"

                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:background="@drawable/ic_right_arrow"
                            android:backgroundTint="@color/black1_20"

                            app:layout_constraintBottom_toBottomOf="@id/tvTodayMagazine"
                            app:layout_constraintStart_toEndOf="@id/tvTodayMagazine"
                            app:layout_constraintTop_toTopOf="@id/tvTodayMagazine" />

                        <com.facebook.shimmer.ShimmerFrameLayout
                            android:id="@+id/sflRecentMagazine"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@id/tvTodayMagazine"
                            app:layout_constraintTop_toBottomOf="@id/tvTodayMagazine">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <include layout="@layout/shimmer_tile_item" />

                                <include layout="@layout/shimmer_tile_item" />

                                <include layout="@layout/shimmer_tile_item" />
                            </LinearLayout>
                        </com.facebook.shimmer.ShimmerFrameLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvRecentMagazine"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="@dimen/side_margin"
                            android:clipToPadding="false"
                            android:clipChildren="false"
                            android:orientation="horizontal"

                            app:layout_constraintBottom_toBottomOf="@+id/sflRecentMagazine"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/sflRecentMagazine" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- Layout 주변 인기치과 레이아웃 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cl_famous_hospital_layout"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/side_margin"
                        android:layout_marginTop="10dp"
                        android:background="@color/white"
                        android:paddingVertical="@dimen/side_margin_2x"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/clDentalMagazine">

                        <TextView
                            android:id="@+id/tv_famous_dental_hospital"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:fontFamily="@font/pretendard_extrabold"
                            android:includeFontPadding="false"
                            android:text="내 주변 인기치과"
                            android:textColor="@color/black1_20"
                            android:textSize="@dimen/H4_M500_18_auto"

                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:background="@drawable/ic_right_arrow"
                            android:backgroundTint="@color/black1_20"

                            app:layout_constraintBottom_toBottomOf="@id/tv_famous_dental_hospital"
                            app:layout_constraintStart_toEndOf="@id/tv_famous_dental_hospital"
                            app:layout_constraintTop_toTopOf="@id/tv_famous_dental_hospital" />

                        <com.facebook.shimmer.ShimmerFrameLayout
                            android:id="@+id/sfl_partner_shimmer"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/tv_famous_dental_hospital">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <include layout="@layout/shimmer_partner_hospital_view" />

                                <include layout="@layout/shimmer_partner_hospital_view" />

                                <include layout="@layout/shimmer_partner_hospital_view" />
                            </LinearLayout>
                        </com.facebook.shimmer.ShimmerFrameLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rv_famous_hospital"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:orientation="vertical"

                            app:layout_constraintBottom_toBottomOf="@id/sfl_partner_shimmer"
                            app:layout_constraintEnd_toEndOf="@id/sfl_partner_shimmer"
                            app:layout_constraintStart_toStartOf="@id/sfl_partner_shimmer"
                            app:layout_constraintTop_toTopOf="@id/sfl_partner_shimmer" />

                        <TextView
                            android:id="@+id/tv_find_near_by_hosptial"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="40dp"
                            android:clickable="true"

                            android:fontFamily="@font/pretendard_extrabold"
                            android:includeFontPadding="false"
                            android:text="가까운 치과찾기"
                            android:textColor="@color/black1_20"
                            android:textSize="@dimen/H4_M500_18_auto"

                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/rv_famous_hospital" />

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:background="@drawable/ic_right_arrow"
                            android:backgroundTint="@color/black1_20"

                            app:layout_constraintBottom_toBottomOf="@id/tv_find_near_by_hosptial"
                            app:layout_constraintStart_toEndOf="@id/tv_find_near_by_hosptial"
                            app:layout_constraintTop_toTopOf="@id/tv_find_near_by_hosptial" />

                        <ImageView
                            android:id="@+id/iv_find_hospital_banner"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:adjustViewBounds="true"
                            android:clickable="true"
                            android:src="@drawable/img_banner_find_dental"

                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tv_find_near_by_hosptial" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <!-- Layout End 주변 인기 치과 레이아웃 -->

                    <!-- Layout 추천 영상 레이아웃 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cl_recommend_video_layout"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:paddingVertical="@dimen/side_margin_2x"
                        android:background="@color/white"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cl_famous_hospital_layout">

                        <TextView
                            android:id="@+id/tv_recommend_video"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/side_margin"
                            android:clickable="true"

                            android:fontFamily="@font/pretendard_extrabold"
                            android:includeFontPadding="false"
                            android:text="추천 영상"
                            android:textColor="@color/black1_20"
                            android:textSize="@dimen/H4_M500_18_auto"

                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:background="@drawable/ic_right_arrow"
                            android:backgroundTint="@color/black1_20"

                            app:layout_constraintBottom_toBottomOf="@id/tv_recommend_video"
                            app:layout_constraintStart_toEndOf="@id/tv_recommend_video"
                            app:layout_constraintTop_toTopOf="@id/tv_recommend_video" />

                        <com.facebook.shimmer.ShimmerFrameLayout
                            android:id="@+id/sfl_recommend_video"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="30dp"
                            android:layout_marginStart="@dimen/side_margin"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/tv_recommend_video">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <include layout="@layout/shimmer_youtube_video_item" />

                                <include layout="@layout/shimmer_youtube_video_item" />
                            </LinearLayout>

                        </com.facebook.shimmer.ShimmerFrameLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rv_recommend_video"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:paddingStart="@dimen/side_margin"
                            android:clipToPadding="false"
                            android:clipChildren="false"
                            android:orientation="horizontal"

                            app:layout_constraintBottom_toBottomOf="@id/sfl_recommend_video"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@id/sfl_recommend_video">

                        </androidx.recyclerview.widget.RecyclerView>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <!-- Layout End 추천 영상 레이아웃 -->

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:paddingHorizontal="@dimen/side_margin"
                        android:paddingTop="@dimen/side_margin"
                        android:paddingBottom="100dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cl_recommend_video_layout">

                        <View
                            android:id="@+id/view_divider"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:background="#ECEDEF"

                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/tv_business_info_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:clickable="true"

                            android:fontFamily="@font/pretendard_bold"
                            android:text="스마일리 사업자정보"
                            android:textColor="@color/gray1_42"
                            android:textSize="@dimen/M500_14_h20"

                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/view_divider" />

                        <ImageView
                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:layout_marginStart="5dp"
                            android:background="@drawable/ic_right_arrow"
                            android:backgroundTint="@color/gray1_42"
                            android:clickable="true"
                            android:rotation="90"

                            app:layout_constraintBottom_toBottomOf="@id/tv_business_info_title"
                            app:layout_constraintStart_toEndOf="@id/tv_business_info_title"
                            app:layout_constraintTop_toTopOf="@id/tv_business_info_title" />

                        <TextView
                            android:id="@+id/tv_business_info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"

                            android:fontFamily="@font/pretendard_regular"
                            android:lineSpacingExtra="3sp"
                            android:text="사업자등록번호: 000-00-00000\n동의대학교 컴퓨터소프트웨어공학과\n부산광역시 부산진구 엄광로 176 정보공학관 915호 (가야동)\n대표전화 : 051-890-0000"
                            android:textColor="@color/gray3_82"
                            android:textSize="@dimen/B4_M500_13_h18"

                            app:layout_constraintStart_toStartOf="@id/tv_business_info_title"
                            app:layout_constraintTop_toBottomOf="@id/tv_business_info_title" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"

                            android:fontFamily="@font/pretendard_medium"
                            android:text="서비스이용약관    |   개인정보처리방침   |    위치기반서비스이용약관     "
                            android:textColor="@color/gray3_82"
                            android:textSize="@dimen/B4_M500_13_h18"

                            app:layout_constraintStart_toStartOf="@+id/tv_business_info"
                            app:layout_constraintTop_toBottomOf="@id/tv_business_info" />

                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <!-- Layout End 콘텐츠 레이아웃 -->
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.core.widget.NestedScrollView>

        </androidx.coordinatorlayout.widget.CoordinatorLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>